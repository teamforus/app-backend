{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.2.0",
        "ext-bcmath": "*",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-zip": "*",
        "astrotomic/laravel-translatable": "11.8.0",
        "aws/aws-sdk-php": "3.133.30",
        "barryvdh/laravel-ide-helper": "2.6.6",
        "bunq/sdk_php": "1.12.1",
        "doctrine/dbal": "2.10.1",
        "dusterio/laravel-aws-worker": "0.1.25",
        "fideloper/proxy": "4.2.2",
        "fruitcake/laravel-cors": "1.0.4",
        "geoip2/geoip2": "2.10.0",
        "graham-campbell/markdown": "12.0.0",
        "guzzlehttp/guzzle": "6.5.2",
        "intervention/image": "2.5.1",
        "kalnoy/nestedset": "5.0.0",
        "laravel-notification-channels/apn": "0.5.0",
        "laravel-notification-channels/fcm": "1.6.0",
        "laravel/framework": "6.10.1",
        "laravel/helpers": "1.1.1",
        "laravel/tinker": "1.0.10",
        "league/flysystem-aws-s3-v3": "1.0.24",
        "maatwebsite/excel": "3.1.19",
        "philkra/elastic-apm-laravel": "6.0.1",
        "predis/predis": "1.1.1",
        "ralouphie/getallheaders": "2.0.5",
        "simplesoftwareio/simple-qrcode": "2.0.0",
        "twilio/sdk": "5.42.2",
        "whichbrowser/parser": "2.0.42"
    },
    "require-dev": {
        "filp/whoops": "2.7.1",
        "fzaninotto/faker": "1.9.1",
        "mockery/mockery": "1.3.1",
        "nunomaduro/collision": "4.1.3",
        "phpunit/phpunit": "7.5.20"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
