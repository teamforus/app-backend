{
    "name": "laravel/laravel",
    "description": "The Laravel Framework.",
    "keywords": ["framework", "laravel"],
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=7.1.0",
        "ext-json": "*",
        "ext-openssl": "*",
        "ext-zip": "*",
        "barryvdh/laravel-cors": "0.11.4",
        "barryvdh/laravel-ide-helper": "2.6.5",
        "bunq/sdk_php": "1.12.1",
        "dimsav/laravel-translatable": "8.1",
        "doctrine/dbal": "2.9.2",
        "fideloper/proxy": "3.3.4",
        "graham-campbell/markdown": "10.3.1",
        "guzzlehttp/guzzle": "6.3.3",
        "intervention/image": "2.5.0",
        "kalnoy/nestedset": "4.3.4",
        "laravel/framework": "5.5.48",
        "laravel/tinker": "1.0.10",
        "laravelcollective/html": "5.5.4",
        "league/flysystem-aws-s3-v3": "1.0.23",
        "maatwebsite/excel": "3.1.17",
        "philkra/elastic-apm-laravel": "5.8.2",
        "ralouphie/getallheaders": "2.0.5",
        "simplesoftwareio/simple-qrcode": "2.0.0",
        "staudenmeir/eloquent-has-many-deep": "1.7"
    },
    "require-dev": {
        "barryvdh/laravel-debugbar": "3.2.8",
        "filp/whoops": "2.5.0",
        "fzaninotto/faker": "1.9.0",
        "mockery/mockery": "1.3.0",
        "phpunit/phpunit": "6.5.14",
        "symfony/thanks": "1.1.0"
    },
    "autoload": {
        "files": [
            "app/helpers.php"
        ],
        "classmap": [
            "database/seeds",
            "database/factories"
        ],
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "dont-discover": [
            ]
        }
    },
    "scripts": {
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate"
        ],
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover"
        ],
        "post-update-cmd": [
            "Illuminate\\Foundation\\ComposerScripts::postUpdate",
            "@php artisan ide-helper:generate",
            "@php artisan ide-helper:meta"
        ]
    },
    "config": {
        "preferred-install": "dist",
        "sort-packages": true,
        "optimize-autoloader": true
    }
}
